{
    "transactions": {
        "slowThresholdMillis": 300,
        "profilingIntervalMillis": 1000,
        "captureThreadStats": true
    },
    "instrumentation": [
        {
            "className": "uk.ac.ucl.rits.inform.datasources.waveform.Hl7MessageCompressor",
            "methodName": "saveMessage",
            "methodParameterTypes": [
                ".."
            ],
            "captureKind": "trace-entry",
            "transactionType": "Web",
            "transactionNameTemplate": "saveMessage",
            "transactionSlowThresholdMillis": 500,
            "alreadyInTransactionBehavior": "capture-new-transaction",
            "traceEntryMessageTemplate": "{{this}}.{{methodName}}({{0}}, {{1}}, {{2}})",
            "timerName": "saveMessage"
        },
        {
            "className": "uk.ac.ucl.rits.inform.datasources.waveform.Hl7ParseAndQueue",
            "methodName": "saveParseQueue",
            "methodParameterTypes": [
                ".."
            ],
            "captureKind": "transaction",
            "transactionType": "Web",
            "transactionNameTemplate": "saveParseQueue",
            "transactionSlowThresholdMillis": 500,
            "alreadyInTransactionBehavior": "capture-new-transaction",
            "traceEntryMessageTemplate": "{{this}}.{{methodName}}({{0}}, {{1}}, {{2}})",
            "timerName": "saveParseQueue"
        }
    ]
}

